{
    "name": "fusic/laravel-sortable",
    "description": "laravel-sortable",
    "license": "MIT",
    "require": {
        "php": "^7.1.3|^7.2|^7.2.5|^7.3|^8.0",
        "laravel/framework":"^6.20.14|^7.24.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sortable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sortable\\Providers\\SortableServiceProvider"
            ]
        }
    }
}
